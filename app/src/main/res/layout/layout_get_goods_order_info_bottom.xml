<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_twelve"
        android:background="@color/color_white"
        android:paddingLeft="@dimen/dimen_twelve"
        android:paddingRight="@dimen/dimen_twelve">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingTop="@dimen/dimen_twelve"
            android:text="@string/get_goods_order_info_amount"
            android:textColor="@color/order_detail_gray_323334"
            android:textSize="@dimen/font_normal" />

        <TextView
            android:id="@+id/tv_activity_get_goods_order_info_goods_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:paddingTop="@dimen/dimen_twelve"
            android:textColor="@color/order_detail_gray_323334"
            tools:text="짜0.00" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_white"
        android:paddingLeft="@dimen/dimen_twelve"
        android:paddingRight="@dimen/dimen_twelve">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingTop="@dimen/dimen_twelve"
            android:text="@string/get_goods_order_info_coupon"
            android:textColor="@color/order_detail_gray_323334"
            android:textSize="@dimen/font_normal" />

        <TextView
            android:id="@+id/tv_activity_get_goods_order_info_coupon_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:paddingTop="@dimen/dimen_twelve"
            android:textColor="@color/order_detail_gray_323334"
            tools:text="-0.00" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_white"
        android:paddingLeft="@dimen/dimen_twelve"
        android:paddingRight="@dimen/dimen_twelve">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingBottom="@dimen/dimen_twelve"
            android:paddingTop="@dimen/dimen_twelve"
            android:text="@string/get_goods_order_info_bag"
            android:textColor="@color/order_detail_gray_323334"
            android:textSize="@dimen/font_normal" />

        <TextView
            android:id="@+id/tv_activity_get_goods_order_info_bag_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:paddingBottom="@dimen/dimen_twelve"
            android:paddingTop="@dimen/dimen_twelve"
            android:textColor="@color/order_detail_gray_323334"
            tools:text="+0.00" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_line"
        android:layout_marginLeft="@dimen/dimen_twelve"
        android:layout_marginRight="@dimen/dimen_twelve"
        android:background="@color/color_background" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_white"
        android:padding="@dimen/dimen_twelve">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/get_goods_order_info_total_amount"
            android:textColor="@color/order_detail_gray_323334"
            android:textSize="@dimen/font_normal" />

        <TextView
            android:id="@+id/tv_activity_get_goods_order_info_all_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:textColor="@color/order_detail_gray_323334"
            android:textSize="@dimen/font_normal"
            tools:text="짜0.00" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_line"
        android:layout_marginLeft="@dimen/dimen_twelve"
        android:layout_marginRight="@dimen/dimen_twelve"
        android:background="@color/color_background" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_white"
        android:paddingLeft="@dimen/dimen_twelve"
        android:paddingRight="@dimen/dimen_twelve">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingTop="@dimen/dimen_twelve"
            android:text="@string/get_goods_order_info_minus"
            android:textColor="@color/green_00d347"
            android:textSize="@dimen/font_normal" />

        <TextView
            android:id="@+id/tv_activity_get_goods_order_info_minus_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:paddingTop="@dimen/dimen_twelve"
            android:textColor="@color/green_00d347"
            android:textSize="@dimen/font_normal"
            tools:text="짜0.00" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_white"
        android:padding="@dimen/dimen_twelve">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/get_goods_order_info_actual_amount"
            android:textColor="@color/preference_red"
            android:textSize="@dimen/font_normal" />

        <TextView
            android:id="@+id/tv_activity_get_goods_order_info_actual_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:textColor="@color/preference_red"
            android:textSize="@dimen/font_normal"
            tools:text="짜0.00" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_white"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_activity_get_goods_order_go_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dimen_twelve"
            android:background="@drawable/shape_yellow_rectangle_stoke_bg2"
            android:gravity="center"
            android:paddingBottom="@dimen/dimen_eight"
            android:paddingTop="@dimen/dimen_eight"
            android:text="@string/get_goods_order_info_look_detail"
            android:textColor="@color/color_yellow_order_details_wait_pick"
            android:textSize="@dimen/font_large" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_twelve"
            android:background="@null" />
    </LinearLayout>
</LinearLayout>